<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.megabrain.javasearchengine.mapper.BookMapper">
    <select id="selectBook" resultType="com.megabrain.javasearchengine.model.Book">
        SELECT  id,
                name,
                author,
                price,
                publication_date
                FROM BOOK
    </select>
</mapper>